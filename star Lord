function setup() {
  createCanvas(600, 600);
}

function draw() {
  background(255,165,0);
  ellipse(300,300,500)
  fill(100,100,100)
 

fill(100,100,100);
  stroke(255);
  strokeWeight(7);

  
  fill(255,0,0)
  beginShape();
  vertex(330,180)
  vertex(250,180)
  vertex(220,95)
  vertex(180,180)
  vertex(100,180)
  vertex(165,235)
  vertex(140,305)
  vertex(215,265)
  vertex(290,305)
  vertex(265,235)
  endShape(CLOSE);
  
 

}
