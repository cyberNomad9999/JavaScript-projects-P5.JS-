var x = 150;
var y = 150;
var xspeed = 5;
var yspeed = 4;

function setup(){
  createCanvas(300,300);
}

function draw(){
  background(64);
  ellipse(x,y,25,25);
  
  x += xspeed;
  y += yspeed;
  
  if(x < 0 || x > width ){
    xspeed *= -1;
  }
  
 if (y < 0 || x > height){
   yspeed *= -1;
 } 
}
